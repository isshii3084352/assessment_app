<!DOCTYPE html>
<!-- body -->
<h1>フォームページ</h1>
<p>
<%= form_for @datum, url: formpage_path do |f| %>
<%= f.label :room_type %>
<%= f.text_field :room_type %>
<br>
<%= f.label :num_rooms %>
<%= f.text_field :num_rooms %>
<br>
<%= f.label :square %>
<%= f.text_field :square %>
<br>
<%= f.label :floor %>
<%= f.text_field :floor %>
<br>
<%= f.label :building_type %>
<%= f.text_field :building_type %>
<br>
<%= f.label :structure_type %>
<%= f.text_field :structure_type %>
<br>
<%= f.label :floor_height %>
<%= f.text_field :floor_height %>
<br>
<%= f.submit "送信"%>
<% end %>
</p>
<!-- /body -->
